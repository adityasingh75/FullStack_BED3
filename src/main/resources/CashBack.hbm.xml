<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
	<class name="legionBank.CashBack" table="customer">  
	          <id name="customerId"></id>  
	          <property name="password"></property>  
	</class>    
	<class name="legionBank.CashBack" table="coupon">  
	          <id name="couponCode"></id>  
	          <property name="offer"></property>  
	</class>   
</hibernate-mapping> 